{
  "dataset_reader": {
    "cache_directory": "cache/new5train",
    "dataset_path": "dataset/database",
    "keep_if_unparsable": false,
    "lazy": false,
    "max_instances": null,
    "question_token_indexers": {
      "tokens": {
        "model_name": "bert-base-uncased",
        "type": "pretrained_transformer"
      }
    },
    "tables_file": "dataset/tables.json",
    "type": "spider_ratsql"
  },
  "train_data_path": "dataset/train_spider.json",
  "data_loader": {
    "batch_size": 10
  },
  "validation_data_loader": {
    "batch_size": 1
  },
  "validation_data_path": "dataset/dev.json",
  "validation_dataset_reader": {
    "cache_directory": "cache/new5val",
    "dataset_path": "dataset/database",
    "keep_if_unparsable": true,
    "lazy": false,
    "max_instances": null,
    "question_token_indexers": {
      "tokens": {
        "model_name": "bert-base-uncased",
        "type": "pretrained_transformer"
      }
    },
    "tables_file": "dataset/tables.json",
    "type": "spider_ratsql"
  },
  "model": {
    "dataset_path": "dataset/",
    "decoder_beam_search": {
      "beam_size": 10
    },
    "dropout": 0.5,
    "input_attention": {
      "type": "dot_product"
    },
    "max_decoding_steps": 100,
    "past_attention": {
      "type": "dot_product"
    },
    "schema_encoder": {
      "action_embedding_dim": 300,
      "dropout": 0.5,
      "parse_sql_on_decoding": true,
      "question_embedder": {
        "token_embedders": {
          "tokens": {
            "model_name": "bert-base-uncased",
            "type": "pretrained_transformer"
          }
        }
      },
      "type": "ratsql"
    },
    "training_beam_size": 1,
    "type": "spider"
  },
  "trainer": {
    "checkpointer": {
      "num_serialized_models_to_keep": 2
    },
    "cuda_device": 0,
    "num_epochs": 1000,
    "optimizer": {
      "lr": 0.001,
      "type": "adam",
      "weight_decay": 0.0005
    },
    "validation_metric": "+sql_match"
  }
}